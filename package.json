{
  "name": "ng2-google-charts",
  "version": "1.0.0",
  "description": "Angular2 Google Charts module",
  "private": true,
  "scripts": {
    "demo.gh-pages": "run-s build demo.build demo.deploy",
    "demo.build": "ng build -prod --aot && npm run generate-bs4",
    "demo.deploy": "gh-pages -d demo/dist",
    "demo-beta-deploy": "gh-pages -d demo/dist -r git@github.com:valorkin/ng2-bootstrap.git -b gh-pages",
    "link": "ngm link -p src --here",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "build": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot",
    "pretest": "run-s lint build link",
    "test": "ng test -sr",
    "test-cross": "SAUCE=true ng test -sr",
    "test-coverage": "ng test -sr -cc"
  },
  "main": "bundles/ng2-google-charts.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular2",
    "ng2",
    "Google",
    "Charts"
  ],
  "author": "Giacomo Mazzamuto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gmazzamuto/ng2-google-charts.git"
  },
  "bugs": {
    "url": "https://github.com/gmazzamuto/ng2-google-charts/issues"
  },
  "homepage": "https://github.com/gmazzamuto/ng2-google-charts",
  "dependencies": {
    "moment": "2.17.1"
  },
  "peerDependencies": {
    "@angular/common": "^2.3.0",
    "@angular/compiler": "^2.3.0",
    "@angular/core": "^2.3.0",
    "@angular/forms": "^2.3.0"
  },
  "devDependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/compiler-cli": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/router": "3.4.3",
    "@angular/tsc-wrapped": "0.5.1",
    "@ngtools/webpack": "1.2.3",
    "@types/jasmine": "2.5.40",
    "@types/webpack": "2.2.0",
    "angular-cli": "1.0.0-beta.25.5",
    "bootstrap": "^4.0.0-alpha",
    "codelyzer": "~2.0.0-beta.4",
    "core-js": "^2.4.1",
    "cpy": "5.0.0",
    "cpy-cli": "1.0.1",
    "del-cli": "0.2.1",
    "gh-pages": "0.12.0",
    "gitignore-to-glob": "0.3.0",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "0.4.0",
    "karma-sauce-launcher": "1.1.0",
    "ngm-cli": "0.4.3",
    "npm-run-all": "4.0.0",
    "reflect-metadata": "0.1.9",
    "require-dir": "0.3.1",
    "rxjs": "5.0.3",
    "systemjs-builder": "0.15.34",
    "ts-helpers": "^1.1.1",
    "ts-node": "2.0.0",
    "tslint": "4.3.1",
    "tslint-config-valorsoft": "1.2.0",
    "typescript": "2.1.5",
    "webdriver-manager": "11.1.1",
    "zone.js": "0.7.5"
  }
}
