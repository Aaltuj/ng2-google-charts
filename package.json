  "description": "Angular2 Google Charts module",
  "main": "build/ng2-google-charts.js",
  "typings": "build/ng2-google-charts.d.ts",
  "files": "build",
  "scripts": {
    "flow:lint": "tslint src/*.ts src/**/*.ts",
    "flow.install:typings": "typings install",
    "flow:compile": "rm -rf build && tsc",
    "build": "npm run flow:lint && npm run flow.install:typings && npm run flow:compile",
    "prepublish": "npm run build",
    "test": "karma start",
    "test:ci": "karma start --singleRun"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gmazzamuto/ng2-google-charts.git"
  },
  "keywords": [
    "angular2",
    "ng2",
    "Google",
    "Charts"
  ],
